@model List<StreetFighter.Dominio.Personagem>
@{
    ViewBag.Title = "ListaDePersonagens";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="titulo">
    <h1>
        Lista de Personagens
    </h1>
</div>
<div class="corpo">    
    <div>
        @{
            foreach (var personagem in Model)
            {
                <span style="margin: 5px; padding: 20px; background-color: red">
                    @personagem.Nome
                </span>
            }
        }
    </div>
</div>
